{
  "version": 2,
  "builds": [
    {
      "src": "my-accuro-website/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "my-accuro-website/build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://backend-98kohxmyv-iynubauhsoj-gmailcoms-projects.vercel.app/api/$1"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://backend-98kohxmyv-iynubauhsoj-gmailcoms-projects.vercel.app/api/$1",
      "check": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/my-accuro-website/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "@api_url"
  }
}
